vector<string> Solution::fizzBuzz(int A) {
    vector<string> ABC;
    for (int i =1 ; i <=A; i++){
        if (i%3 == 0 && i%5 == 0){
            ABC.push_back("FizzBuzz");
        }
        else if (i%3 == 0)ABC.push_back("Fizz");
        else if (i%5 == 0)ABC.push_back("Buzz");
        else {ABC.push_back(to_string(i));}
    }
    return ABC;
}
